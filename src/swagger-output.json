                },
                "apellido": {
                  "type": "string",
                  "example": "Perez"
                },
                "domicilio": {
                  "type": "string",
                  "example": "123456789"
                },
                "tipo": {
                  "type": "string",
                  "example": "Particular"
                },
                "usuario": {
                  "type": "string",
                  "example": "CharlyP@example.com"
                },
                "clave": {
                  "type": "string",
                  "example": "@Charly1234"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "201": {
            "description": "Created"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal Server Error"
